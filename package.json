{
	"name": "@jozefini/tailwindcss",
	"version": "0.0.5",
	"description": "A Tailwind CSS plugin for that adds missing utility classes",
	"author": "Jozefin B. <jozi.me>",
	"keywords": [
		"tailwindcss",
		"tailwindcss-plugin"
	],
	"main": "src/index.js",
	"types": "src/index.d.ts",
	"files": [
		"src/*.js",
		"src/*.d.ts",
		"src/styles/*.js",
		"dist/"
	],
	"repository": "https://github.com/jozefini/tailwindcss-plugin",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"test": "node src/test.js",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"peerDependencies": {
		"tailwindcss": ">=3.0.0 || insiders"
	},
	"devDependencies": {
		"autoprefixer": "^10.2.1",
		"postcss": "^8.2.3",
		"prettier": "^2.1.2",
		"tailwindcss": "^3.2.2"
	},
	"dependencies": {}
}
